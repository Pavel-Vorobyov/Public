        <attribute>
            <name>courseCourse</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>

        <attribute>
            <name>courseDescription</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>

    <tag>
        <name>taskLine</name>
        <tag-class>by.vorobyov.training.tag.TaskLine</tag-class>
        <body-content>empty</body-content>

        <attribute>
            <name>taskHerf</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>

        <attribute>
            <name>taskTitle</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>

        <attribute>
            <name>startTime</name>
