            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>

    <tag>
        <name>userTaskLine</name>
        <tag-class>by.vorobyov.training.tag.UserTaskLine</tag-class>
        <body-content>empty</body-content>

        <attribute>
            <name>blockUpdateId</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>

        <attribute>
            <name>name</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>

        <attribute>
            <name>creationTime</name>
